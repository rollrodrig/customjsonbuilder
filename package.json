{
  "name": "customjsonbuilder",
  "version": "2.0.5",
  "description": "Fake json responses with custom fields on the fly",
  "main": "index.js",
  "scripts": {
    "start": "node dist/app.js",
    "watch": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app.ts",
    "build": "./node_modules/.bin/tsc",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix",
    "prepare": "husky",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,json,md}'",
    "dev": "./node_modules/.bin/ts-node src/index.ts",
    "test": "jest"
  },
  "types": "./types/customjsonbuilder/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rollrodrig/customjsonbuilder.git"
  },
  "author": "rollrodrig",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rollrodrig/customjsonbuilder/issues"
  },
  "homepage": "https://github.com/rollrodrig/customjsonbuilder#readme",
  "dependencies": {
    "@faker-js/faker": "^8.4.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint 'src/**/*.{ts,tsx}' --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.3.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/moment": "^2.13.0",
    "@types/node": "^20.14.9",
    "@types/sinon": "^17.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "chai": "^5.1.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.4.0",
    "express": "^4.19.2",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "sinon": "^18.0.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}